{
  "name": "libsvm-js",
  "version": "0.0.0",
  "description": "A port of libsvm to javascript using emscripten",
  "main": "src/SVM.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "mocha --require should --recursive test",
    "demo-build": "rimraf demo-dist && BABEL_ENV=production webpack -p",
    "serve": "webpack-dev-server",
    "deploy": "npm run demo-build && cp demos/index.html demo-dist/ && gh-pages -d demo-dist",
    "build": "rimraf dist && WASM=1 make; make clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mljs/libsvm.git"
  },
  "keywords": [
    "svm",
    "libsvm",
    "machine",
    "learning",
    "support",
    "vector",
    "machines"
  ],
  "author": "Daniel Kostro <kostro.d@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/mljs/libsvm/issues"
  },
  "homepage": "https://github.com/mljs/libsvm#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "bootstrap": "4.0.0-alpha.6",
    "chroma-js": "^1.2.2",
    "css-loader": "^0.27.1",
    "file-loader": "^0.10.1",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "^0.0.5-beta.2",
    "gh-pages": "^0.12.0",
    "less": "^2.7.2",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "mocha": "^3.2.0",
    "mocha-better-spec-reporter": "^3.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "beta",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-undo": "1.0.0-beta9-7",
    "reselect": "^2.5.4",
    "rimraf": "^2.6.1",
    "should": "^11.2.0",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
