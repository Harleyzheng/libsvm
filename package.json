{
  "name": "libsvm-js",
  "version": "0.0.0",
  "description": "A port of libsvm to javascript using emscripten",
  "main": "wasm.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "build": "rimraf dist && make; make clean",
    "demo-build": "rimraf demo-dist && BABEL_ENV=production webpack -p --config webpack.production.js && cp demos/index.html demo-dist",
    "deploy": "npm run demo-build && gh-pages -d demo-dist",
    "eslint": "eslint src demos",
    "eslint-fix": "eslint --fix src demos",
    "serve": "webpack-dev-server",
    "test": "mocha --require should --recursive test",
    "build-benchmark": "(cd benchmark/iris; make)",
    "benchmark": "npm run build-benchmark && (node benchmark/iris/iris-cv.js asm; node benchmark/iris/iris-cv.js wasm; cd benchmark/iris && ./iris-cv)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mljs/libsvm.git"
  },
  "keywords": [
    "svm",
    "libsvm",
    "machine",
    "learning",
    "support",
    "vector",
    "machines"
  ],
  "author": "Daniel Kostro <kostro.d@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/mljs/libsvm/issues"
  },
  "homepage": "https://github.com/mljs/libsvm#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "bootstrap": "4.0.0-alpha.6",
    "chroma-js": "^1.2.2",
    "css-loader": "^0.27.1",
    "eslint": "^3.18.0",
    "eslint-config-cheminfo": "^1.7.0",
    "eslint-config-neptun-react": "^1.0.1",
    "eslint-plugin-no-only-tests": "^1.1.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.10.1",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "^0.0.5-beta.2",
    "gh-pages": "^0.12.0",
    "imports-loader": "^0.7.1",
    "jquery": "^3.2.1",
    "less": "^2.7.2",
    "lodash.countby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "lodash.mapvalues": "^4.6.0",
    "lodash.reduce": "^4.6.0",
    "lodash.throttle": "^4.1.1",
    "ml-dataset-iris": "^1.0.0",
    "mocha": "^3.2.0",
    "mocha-better-spec-reporter": "^3.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "beta",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-undo": "1.0.0-beta9-7",
    "reselect": "^2.5.4",
    "rimraf": "^2.6.1",
    "should": "^11.2.0",
    "style-loader": "^0.13.2",
    "tether": "^1.4.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
